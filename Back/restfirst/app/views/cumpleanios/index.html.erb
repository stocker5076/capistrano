<p id="notice"><%= notice %></p>

<h1>Cumpleanios</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cumpleanios.each do |cumpleanio| %>
      <tr>
        <td><%= cumpleanio.fecha %></td>
        <td><%= cumpleanio.nombre %></td>
        <td><%= link_to 'Show', cumpleanio %></td>
        <td><%= link_to 'Edit', edit_cumpleanio_path(cumpleanio) %></td>
        <td><%= link_to 'Destroy', cumpleanio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cumpleanio', new_cumpleanio_path %>
